GENERATED_DIRTY += ReadGet_Test_BaseS.h ReadGet_Test_BaseC.h ReadGet_Test_Base_stub_export.h ReadGet_Test_BaseC.cpp
OBJS_DEPEND_ON_GENERATED = 1
## More than one file is generated by the command and therefore
## it can not be run in parallel.  Unfortunately, there is no way to
## say that only this rule can't be run in parallel.  However, we can
## determine if the generated files have already been generated.  If that's
## the case, then we don't need this special rule.
ifeq ($(wildcard $(GENERATED_DIRTY)), $(GENERATED_DIRTY))
  ## If we can find /bin/test, then we will continue
  ifneq ($(TEST_EXE),)
    ## If all of the generated files are there, then we need to check
    ## and make sure that the generated files are up-to-date.  If they are not
    ## then we need the special rule.
    ifeq ($(shell $(TEST_EXE) "ReadGet_Test_Base.idl" -nt "ReadGet_Test_BaseS.h" 2> /dev/null || $(TEST_EXE) "ReadGet_Test_Base.idl" -nt "ReadGet_Test_BaseC.h" 2> /dev/null || $(TEST_EXE) "ReadGet_Test_Base.idl" -nt "ReadGet_Test_Base_stub_export.h" 2> /dev/null || $(TEST_EXE) "ReadGet_Test_Base.idl" -nt "ReadGet_Test_BaseC.cpp" 2> /dev/null || echo 0),)
      .NOTPARALLEL:

    else
      ## By this point, all of the generated files are here and up-to-date
      ## with respect to the source file.  Now we need to make sure that
      ## they are up-to-date with respect to the generation tool.  If the tool
      ## is newer than the generated files, then we need the special rule.
      ifeq ($(shell $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "ReadGet_Test_BaseS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "ReadGet_Test_BaseC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "ReadGet_Test_Base_stub_export.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "ReadGet_Test_BaseC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "ReadGet_Test_BaseS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "ReadGet_Test_BaseC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "ReadGet_Test_Base_stub_export.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "ReadGet_Test_BaseC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "ReadGet_Test_BaseS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "ReadGet_Test_BaseC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "ReadGet_Test_Base_stub_export.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "ReadGet_Test_BaseC.cpp" 2> /dev/null || echo 0),)
        .NOTPARALLEL:
      endif
    endif
  else
    .NOTPARALLEL:
  endif
else
.NOTPARALLEL:
endif
ReadGet_Test_BaseS.h ReadGet_Test_BaseC.h ReadGet_Test_Base_stub_export.h ReadGet_Test_BaseC.cpp: ReadGet_Test_Base.idl $(subst $(SPACE),\$(SPACE),$(call ADD_ARCH,$(TAO_IDL_DEP)))
	$(if $(findstring ",$(TAO_IDL)),$(TAO_IDL),$(call ADD_QUOTE,$(call ADD_ARCH,$(TAO_IDL)))) $(TAO_IDLFLAGS) -Sa -SS -Sci -Wb,stub_export_macro=QUERY_CONDITION_TEST_BASE_STUB_Export -Wb,stub_export_include=ReadGet_Test_Base_stub_export.h -Gxhst ReadGet_Test_Base.idl


ifneq ($(static_libs_only),1)
ifneq ($(CROSS-COMPILE),1)
ReadGet_Test_BaseS.h ReadGet_Test_BaseC.h ReadGet_Test_Base_stub_export.h ReadGet_Test_BaseC.cpp: $(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT)) $(call ADD_ARCH,../../../../../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))
endif
endif

