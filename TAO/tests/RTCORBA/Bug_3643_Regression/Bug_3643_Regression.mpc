project(*idl): taoidldefaults {
  idlflags += -Sp
  IDL_Files {
    test.idl
  }
  custom_only = 1
}

project(*Server): rt_server, avoids_minimum_corba, avoids_corba_e_compact, avoids_corba_e_micro {
  after += *idl
  Source_Files {
    test_i.cpp
    server.cpp
  }
  Source_Files {
    testC.cpp
    testS.cpp
  }
  IDL_Files {
  }
}

project(*Client): rt_client {
  after += *idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    testC.cpp
  }
  IDL_Files {
  }
}

