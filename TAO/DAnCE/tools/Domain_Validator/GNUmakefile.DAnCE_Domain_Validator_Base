GENERATED_DIRTY += Domain_ValidatorC.inl Domain_ValidatorS.h Domain_ValidatorC.h Domain_ValidatorS.cpp Domain_ValidatorC.cpp DAnCE_Domain_Validator_stub_export.h
OBJS_DEPEND_ON_GENERATED = 1
## More than one file is generated by the command and therefore
## it can not be run in parallel.  Unfortunately, there is no way to
## say that only this rule can't be run in parallel.  However, we can
## determine if the generated files have already been generated.  If that's
## the case, then we don't need this special rule.
ifeq ($(wildcard $(GENERATED_DIRTY)), $(GENERATED_DIRTY))
  ## If we can find /bin/test, then we will continue
  ifneq ($(TEST_EXE),)
    ## If all of the generated files are there, then we need to check
    ## and make sure that the generated files are up-to-date.  If they are not
    ## then we need the special rule.
    ifeq ($(shell $(TEST_EXE) "Domain_Validator.idl" -nt "Domain_ValidatorC.inl" 2> /dev/null || $(TEST_EXE) "Domain_Validator.idl" -nt "Domain_ValidatorS.h" 2> /dev/null || $(TEST_EXE) "Domain_Validator.idl" -nt "Domain_ValidatorC.h" 2> /dev/null || $(TEST_EXE) "Domain_Validator.idl" -nt "Domain_ValidatorS.cpp" 2> /dev/null || $(TEST_EXE) "Domain_Validator.idl" -nt "Domain_ValidatorC.cpp" 2> /dev/null || $(TEST_EXE) "Domain_Validator.idl" -nt "DAnCE_Domain_Validator_stub_export.h" 2> /dev/null || echo 0),)
      .NOTPARALLEL:

    else
      ## By this point, all of the generated files are here and up-to-date
      ## with respect to the source file.  Now we need to make sure that
      ## they are up-to-date with respect to the generation tool.  If the tool
      ## is newer than the generated files, then we need the special rule.
      ifeq ($(shell $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Domain_ValidatorC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Domain_ValidatorS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Domain_ValidatorC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Domain_ValidatorS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "Domain_ValidatorC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,$(TAO_IDL_DEP))" -nt "DAnCE_Domain_Validator_stub_export.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Domain_ValidatorC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Domain_ValidatorS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Domain_ValidatorC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Domain_ValidatorS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "Domain_ValidatorC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_BE.$(SOEXT))" -nt "DAnCE_Domain_Validator_stub_export.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Domain_ValidatorC.inl" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Domain_ValidatorS.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Domain_ValidatorC.h" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Domain_ValidatorS.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)TAO_IDL_FE.$(SOEXT))" -nt "Domain_ValidatorC.cpp" 2> /dev/null || $(TEST_EXE) "$(call ADD_ARCH,../../../../lib/$(LIB_PREFIX)