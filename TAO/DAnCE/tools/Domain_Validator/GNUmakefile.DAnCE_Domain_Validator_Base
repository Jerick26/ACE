        .NOTPARALLEL:
      endif
    endif
  else
    .NOTPARALLEL:
  endif
else
.NOTPARALLEL:
endif
Domain_ValidatorC.inl Domain_ValidatorS.h Domain_ValidatorC.h Domain_ValidatorS.cpp Domain_ValidatorC.cpp DAnCE_Domain_Validator_stub_export.h: Domain_Validator.idl $(subst $(SPACE),\$(SPACE),$(call ADD_ARCH,$(TAO_IDL_DEP)))
